{
  "name": "AI Organizer Bot",
  "url": "https://github.com/issdandavis",
  "description": "An AI-powered assistant that organizes issues, repositories, and workflows across the ISDanDavis enterprise. Automatically creates AI agents, manages tasks, and provides comprehensive GitHub automation.",
  "hook_attributes": {
    "url": "https://REPLACE_WITH_YOUR_WEBHOOK_URL/github",
    "active": true
  },
  "redirect_url": "https://REPLACE_WITH_YOUR_DOMAIN/auth/callback",
