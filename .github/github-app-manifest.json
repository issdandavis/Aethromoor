    "https://REPLACE_WITH_YOUR_DOMAIN/auth/callback"
  ],
  "setup_url": "https://REPLACE_WITH_YOUR_DOMAIN/ai-bot/setup",
  "setup_on_update": true,
  "public": false,
  "default_permissions": {
    "actions": "write",
    "administration": "write",
    "checks": "write",
    "contents": "write",
    "deployments": "write",
    "discussions": "write",
    "environments": "write",
    "issues": "write",
    "members": "write",
    "metadata": "read",
